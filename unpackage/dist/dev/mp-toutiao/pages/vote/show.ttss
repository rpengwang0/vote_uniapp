@charset "UTF-8";
/**
 * 下方引入的为uView UI的集成样式文件，为scss预处理器，其中包含了一些"u-"开头的自定义变量
 * 使用的时候，请将下面的一行复制到您的uniapp项目根目录的uni.scss中即可
 * uView自定义的css类名和scss变量，均以"u-"开头，不会造成冲突，请放心使用 
 */
.container .title {
  padding: 50rpx 0 30rpx 0;
  text-align: center;
  color: #f96770;
  font-size: 36rpx;
  font-weight: bold;
}
.container .uni-margin-wrap .swiper {
  height: 400rpx;
}
.container .uni-margin-wrap .swiper .swiper-item, .container .uni-margin-wrap .swiper .swiper-item image {
  width: 100%;
  height: 100%;
}
.container .enrollBox {
  display: flex;
  text-align: center;
  align-content: center;
  justify-content: center;
}
.container .enrollBox .enroll-btn {
  width: 250rpx;
  height: 60rpx;
  line-height: 60rpx;
  color: #fff;
  background-color: #f96770;
  border-radius: 50rpx;
  margin-top: 30rpx;
}
.container .downBox {
  background-color: #f5f5f5;
  display: flex;
  text-align: center;
  flex-wrap: wrap;
  align-content: center;
  margin: 0 50rpx;
  border-radius: 15rpx;
}
.container .downBox view {
  flex-basis: 100%;
}
.container .downBox .status {
  color: #f96770;
  padding: 20rpx 0;
  letter-spacing: 4rpx;
  font-weight: 500;
}
.container .downBox .udown {
  padding: 10rpx 0 20rpx 0;
}
.container .numberBox, .container .orderBox {
  display: flex;
  justify-content: center;
  padding: 50rpx;
  align-content: center;
  text-align: center;
}
.container .numberBox view, .container .orderBox view {
  flex: 1;
}
.container .numberBox .number, .container .orderBox .number {
  color: #f96770;
  font-size: 50rpx;
  font-weight: bold;
  padding-bottom: 10rpx;
}
.container .numberBox .numberText, .container .orderBox .numberText {
  color: #909399;
}
.container .searchBox {
  padding: 10rpx 50rpx;
}
.container .orderBox {
  padding: 30rpx;
  font-size: 30rpx;
}
.container .orderBox .thisOrder {
  font-size: 34rpx;
  color: #f96770;
}
.container .list .list-box {
  display: flex;
  justify-content: space-between;
  flex-wrap: wrap;
  padding: 0 20rpx;
}
.container .list .warter {
  border-radius: 16rpx;
  background-color: #ffffff;
  position: relative;
}
.container .list .warter .list-id {
  position: absolute;
  top: 20rpx;
  right: 0rpx;
  background: rgba(143, 143, 143, 0.4);
  border-radius: 0 16rpx 0 40px;
  color: #fff;
  min-width: 60rpx;
  text-align: center;
  padding: 4rpx;
}
.container .list .warter .list-pm {
  position: absolute;
  color: #fff;
  bottom: 80rpx;
  left: 0rpx;
  right: 0rpx;
  display: flex;
  flex-wrap: nowrap;
  justify-content: space-between;
  padding: 10rpx 20rpx;
  border-radius: 0 0 16rpx 16rpx;
  background: linear-gradient(to bottom, rgba(0, 0, 0, 0.01), rgba(0, 0, 0, 0.3));
}
.container .list .warter .list-bottom {
  color: #333;
  display: flex;
  flex-wrap: nowrap;
  justify-content: space-between;
  padding: 10rpx 20rpx;
}
.container .list .warter .list-name {
  flex-basis: 60%;
  word-break: break-all;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 1;
  overflow: hidden;
}
.container .list .left-warter {
  padding: 20rpx 0;
  width: 48.5%;
}
.container .list .right-warter {
  padding: 20rpx 20rpx 20rpx 10rpx;
}
.container .list .right-warter .list-id {
  right: 20rpx;
}
.container .list .right-warter .list-bottom {
  left: 10rpx;
  right: 20rpx;
}
.container .page-bottom {
  position: fixed;
  bottom: 0;
  left: 0;
  right: 0;
  z-index: 95;
  display: flex;
  justify-content: space-between;
  align-items: flex-end;
  background-color: #fff;
  border-top: 1px solid #f5f5f5;
}
.container .page-bottom .btn-left {
  border: 1px solid #f96770;
  color: #f96770;
  text-align: center;
  height: 80rpx;
  line-height: 80rpx;
  border-radius: 50rpx;
  flex: 1;
  margin: 20rpx 20rpx 30rpx 20rpx;
}
.container .page-bottom .btn-right {
  color: #fff;
  background-image: linear-gradient(#f96770, #fc587d);
  text-align: center;
  height: 80rpx;
  line-height: 80rpx;
  border-radius: 50rpx;
  flex: 1;
  margin: 20rpx 20rpx 30rpx 20rpx;
}
.body {
  padding-bottom: 120rpx;
}
.udown .u-countdown .u-countdown-colon {
  padding-right: 20rpx;
}

